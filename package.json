{
  "name": "trigrams",
  "version": "1.0.0",
  "description": "Trigram files for 400+ languages",
  "license": "MIT",
  "keywords": [
    "universal",
    "declaration",
    "human",
    "rights",
    "unicode",
    "udhr",
    "trigram",
    "language",
    "detection",
    "guessing"
  ],
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/trigrams.git"
  },
  "bugs": "https://github.com/wooorm/trigrams/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js",
    "data"
  ],
  "devDependencies": {
    "istanbul": "^0.4.0",
    "markdown-table": "^0.4.0",
    "mocha": "^2.0.0",
    "trigram-utils": "^0.1.0",
    "udhr": "^1.0.0",
    "xo": "^0.18.1"
  },
  "scripts": {
    "build-data": "node script/build-data.js",
    "build-support": "node script/build-support.js",
    "build": "npm run build-data && npm run build-support",
    "lint": "xo",
    "test-api": "_mocha --reporter spec --check-leaks -u exports test.js",
    "test-coverage": "istanbul cover _mocha -- -- test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "xo": {
    "space": true,
    "esnext": false,
    "rules": {
      "guard-for-in": "off"
    },
    "ignores": [
      "data/*.js"
    ]
  }
}
